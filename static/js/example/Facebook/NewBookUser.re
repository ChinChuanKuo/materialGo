                bottomRight="12"
                bottomLeft="12">
                <GridContainer
                  backgroundColor="transparent"
                  direction="row"
                  justify="between"
                  alignItem="center">
                  <GridItem
                    top="0"
                    right="0"
                    bottom="0"
                    left="0"
                    enterBackgroundColor="transparent"
                    backgroundColor="transparent"
                    xs="no">
                    <GridContainer
                      backgroundColor="transparent"
                      direction="row"
                      justify="start"
                      alignItem="center">
                      <GridItem
                        top="0"
                        bottom="0"
                        left="0"
                        enterBackgroundColor="transparent"
                        backgroundColor="transparent"
                        xs="no">
                        <IconButton
                          backgroundColor="rgba(0,0,0,0.08)" padding="6">
                          <IconAction
                            animation="leftRight"
                            src=nightsStayBlack
                          />
                        </IconButton>
                      </GridItem>
                      <GridItem
                        top="0"
                        right="0"
                        bottom="0"
                        left="0"
                        enterBackgroundColor="transparent"
                        backgroundColor="transparent"
                        xs="no">
                        <Typography variant="subheading">
                          <FormattedMessage
                            id="Face.night"
                            defaultMessage="Night"
                          />
                        </Typography>
                      </GridItem>
                    </GridContainer>
                  </GridItem>
                  <GridItem
                    top="0"
                    right="0"
                    bottom="0"
                    left="0"
                    enterBackgroundColor="transparent"
                    backgroundColor="transparent"
                    xs="no">
                    <Switch
                      right="0"
                      checked={checked |> disabledObjects}
                      circleColor={
                        checked |> disabledObjects |> SwitchColor.circle
                      }
                      linearColor={
                        checked |> disabledObjects |> SwitchColor.linear
                      }
                      fontColor={
                        checked |> disabledObjects |> SwitchColor.font
                      }
                      onClick=?switchChecked>
                      null
                    </Switch>
                  </GridItem>
                </GridContainer>
              </MenuItem>
              <MenuItem
                top="7"
                right="0"
                bottom="7"
                left="8"
                topLeft="12"
                topRight="12"
                bottomRight="12"
                bottomLeft="12"
                onClick=?clickOut>
                <GridContainer
                  backgroundColor="transparent"
                  direction="row"
                  justify="start"
                  alignItem="center">
                  <GridItem
                    top="0"
                    bottom="0"
                    left="0"
                    enterBackgroundColor="transparent"
                    backgroundColor="transparent"
                    xs="no">
                    <IconButton backgroundColor="rgba(0,0,0,0.08)" padding="6">
                      <IconAction animation="leftRight" src=inputBlack />
                    </IconButton>
                  </GridItem>
                  <GridItem
                    top="0"
                    right="0"
                    bottom="0"
                    left="0"
                    enterBackgroundColor="transparent"
                    backgroundColor="transparent"
                    xs="no">
                    <Typography variant="subheading">
                      <FormattedMessage
                        id="Action.logout"
                        defaultMessage="Logout"
                      />
                    </Typography>
                  </GridItem>
                </GridContainer>
              </MenuItem>
            </GridItem>
          </GridContainer>
        </Menu>
      </GridItem>
    </div>
    <BackgroundBoard
      showBackground=true
      backgroundColor="transparent"
      onClick=?clickShow
    />
  </>;
};
