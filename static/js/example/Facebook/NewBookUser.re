type state = {
  showPrivacy: bool,
  showSupport: bool,
};

type action =
  | ShowPrivacy
  | ShowSupport;

let reducer = (state, action) =>
  switch (action) {
  | ShowPrivacy => {showPrivacy: !state.showPrivacy, showSupport: false}
  | ShowSupport => {showSupport: !state.showSupport, showPrivacy: false}
  };

let initialState = {showPrivacy: false, showSupport: false};

let transforms = showAnimation =>
  showAnimation ? "translate(0px, 0px)" : "translateX(100vh) translateX(0px)";

let boardstyles = showAnimation =>
  ReactDOMRe.Style.make(
    ~position="absolute",
    ~transform={
      showAnimation |> transforms;
    },
    ~transition="transform 225ms cubic-bezier(0, 0, 0.2, 1) 0ms",
    (),
  );

